# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'WebViewAppDemo' do

  # Comment the next line if you don't want to use dynamic frameworks
  platform :ios, '13.0'
  use_frameworks!

  # Pods for WebViewAppDemo

  # UUID + KeyChain
  pod 'TAKUUID'

  # AutoLayout Support
  pod 'SnapKit', '~> 5.6.0'

  # ProgressDialog
  pod 'ProgressHUD'
  pod 'RappleProgressHUD'

  # SnackBar (like Toast)
  pod 'MaterialComponents/Snackbar'

  # Gif Support
  pod 'SwiftyGif'

  # PhotoImage Picker
  pod 'BSImagePicker', '~> 3.3.1'

end
